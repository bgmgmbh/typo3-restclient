{
  "name": "ts/restclient",
  "type": "typo3-cms-extension",
  "description": "A client to consume RESTful Web Services",
  "homepage": "https://typo3.org/extensions/repository/view/restclient",
  "authors": [
    {
      "name": "Tommaso Sacramone",
      "email": "tommasosacramone.www@gmail.com"
    }
  ],
  "require": {
    "typo3/cms-core": "12.4.*"
  },
  "autoload": {
    "psr-4": {
      "TS\\Restclient\\": "Classes"
    }
  },
  "keywords": [
    "restclient",
    "rest",
    "client",
    "restful",
    "http",
    "webservices"
  ],
  "support": {
    "issues": "https://forge.typo3.org/projects/extension-restclient"
  },
  "version": "12.4.0",
  "extra": {
    "typo3/cms": {
      "extension-key": "restclient"
    }
  },
  "replace": {
    "restclient": "self.version",
    "typo3-ter/restclient": "self.version"
  }
}
